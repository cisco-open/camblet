#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bertab/Work/nasp-blog/node_modules/.pnpm/tsm@2.3.0/node_modules/tsm/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/tsm@2.3.0/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bertab/Work/nasp-blog/node_modules/.pnpm/tsm@2.3.0/node_modules/tsm/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/tsm@2.3.0/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tsm@2.3.0/node_modules/tsm/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../tsm@2.3.0/node_modules/tsm/bin.js" "$@"
fi
