#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bertab/Work/nasp-blog/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules/@astrojs/language-server/bin/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules/@astrojs/language-server/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules/@astrojs/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bertab/Work/nasp-blog/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules/@astrojs/language-server/bin/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules/@astrojs/language-server/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules/@astrojs/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules/@astrojs/language-server/bin/nodeServer.js" "$@"
else
  exec node  "$basedir/../../../../../../@astrojs+language-server@2.5.2_prettier@3.1.0_typescript@5.1.6/node_modules/@astrojs/language-server/bin/nodeServer.js" "$@"
fi
