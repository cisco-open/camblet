#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bertab/Work/nasp-blog/node_modules/.pnpm/astro@2.9.7_@types+node@18.18.10_sass@1.69.5_sharp@0.32.4/node_modules/astro/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/astro@2.9.7_@types+node@18.18.10_sass@1.69.5_sharp@0.32.4/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bertab/Work/nasp-blog/node_modules/.pnpm/astro@2.9.7_@types+node@18.18.10_sass@1.69.5_sharp@0.32.4/node_modules/astro/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/astro@2.9.7_@types+node@18.18.10_sass@1.69.5_sharp@0.32.4/node_modules:/Users/bertab/Work/nasp-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@2.9.7_@types+node@18.18.10_sass@1.69.5_sharp@0.32.4/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@2.9.7_@types+node@18.18.10_sass@1.69.5_sharp@0.32.4/node_modules/astro/astro.js" "$@"
fi
